{
    "manifest_version" : 2,
    "name" : "MealTime",
    "description" : "Watch documentaries/short videos while you eat",
    "version" : "1.0.0" ,
    "icons" : {"128": "icon_128.png"},
    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup": "popup.html"
    },
    
    "permissions" : ["activeTab","tabs"]
}